{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Remote Launch - Framework-dependent",
            "type": "coreclr",
            "request": "launch",
            "program": "dotnet",
            "args": ["/Apps/AlarmClock/AlarmClockWeb/bin/Debug/net7.0/linux-arm64/AlarmClockWeb.dll"],
            "cwd": "/Apps/AlarmClock/AlarmClockWeb",
            "stopAtEntry": false,
            "console": "internalConsole",
            "pipeTransport": {
                "pipeCwd": "${workspaceRoot}",
                "pipeProgram": "ssh",
                "pipeArgs": [
                    "root@devpi.lan"
                ],
                "debuggerPath": "/root/vsdbg/vsdbg"
                }
            },
    ]
}